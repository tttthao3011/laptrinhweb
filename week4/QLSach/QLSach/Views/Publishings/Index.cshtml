@model IEnumerable<QLSach.Models.Publishing>

@{
    ViewBag.Title = "Index";
}

<h2>Nhà xuất bản</h2>

<p>
    @Html.ActionLink("Thêm nhà xuất bản", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.Label("Tên nhà xuất bản")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PublishingName)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id=item.PublishingId }) |
            @Html.ActionLink("Chi tiết", "Details", new { id=item.PublishingId }) |
            @Html.ActionLink("Xóa", "Delete", new { id=item.PublishingId })
        </td>
    </tr>
}

</table>
